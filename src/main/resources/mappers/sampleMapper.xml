<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="sample">
	<select id="sampleList" resultType="com.edu.myapp.dto.SampleDTO">
		select * from sample_le
	</select>
	<select id="getSample" resultType="com.edu.myapp.dto.SampleDTO">
		select * from sample_le where id=#{id}
	</select>
</mapper>